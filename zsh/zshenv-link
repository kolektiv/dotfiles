# load version string comparison
autoload is-at-least
if is-at-least "4.3.17" $ZSH_VERSION ; then
else
    [[ -x ~/programs/${SYS_TYPE}/bin/zsh ]] && exec ~/programs/${SYS_TYPE}/bin/zsh
fi
# ZDOTDIR=~/.zsh/prezto/runcoms
ZDOTDIR=~/.zsh
[[ ! -d ~/.tmp ]] && mkdir ~/.tmp
[[ "$TERM_PROGRAM" == "DTerm" ]] || source $ZDOTDIR/.zshenv
[[ "$TERM_PROGRAM" == "DTerm" ]] && export PATH=/opt/local/bin:$PATH

