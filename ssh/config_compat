Host localhost
    ControlMaster no

# Host rz-stash.llnl.gov
#     ControlMaster no
#     User git
#     ForwardAgent no

Host *
    ForwardAgent yes
    ServerAliveInterval 15
    ServerAliveCountMax 3
    ControlPath ~/.ssh/master-%r@%h:%p
    ControlMaster auto
    ControlPersist 12h
    PermitLocalCommand yes
    SendEnv LC_TERM_PROGRAM LC_ITERM_PROFILE

