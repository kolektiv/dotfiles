Host !rz-stash*
    User scogland
    ForwardAgent yes
    ForwardX11 yes
    ServerAliveInterval 15
    ServerAliveCountMax 3
    ControlPath ~/.ssh/master-%r@%h:%p
    ControlMaster auto
    ControlPersist 12h
    PermitLocalCommand yes
    SendEnv LC_TERM_PROGRAM LC_ITERM_PROFILE

